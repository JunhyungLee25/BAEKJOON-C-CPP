#include<iostream>
using namespace std;

int main(){
    int s[8];
    int asc = 0;
    int des = 0;
    for(int i=0; i<8; i++){
        cin >> s[i];
        if( s[i] == i+1){asc++;}
        else if( s[i] == 8-i){des++;}
    }
    if(asc == 8){
        cout << "ascending";
    }
    else if(des == 8){
        cout << "descending";
    }
    else {cout << "mixed";}
    return 0;
}
