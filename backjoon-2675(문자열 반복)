#include<iostream>
#include<string>
using namespace std;

int main(){
    int t;
    cin >> t;
    int R;
    string s;
    for(int i=0; i<t; i++){
        cin >> R;
        cin >> s;
        for(int j=0; j<s.length(); j++){
            for(int k=0; k<R; k++){
                cout << s[j];
            }
        }
        cout << '\n';
    }
}
