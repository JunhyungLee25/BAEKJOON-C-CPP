#include<iostream>
using namespace std;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(NULL);
    int N;
    cin >> N;
    int input;
    int minvalue = 1000001;
    int maxvalue = -1000001;
    for(int i=0; i<N; i++){
        cin >> input;
        if(input > maxvalue){
            maxvalue = input;
        }
        if(input < minvalue){
            minvalue = input;
        }
    }
    cout << minvalue << " " << maxvalue;
    return 0;
}
