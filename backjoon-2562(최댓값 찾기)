#include<iostream>
using namespace std;
 
int main(){
    int A[100];
    int maxval = 0;
    int s = 0;
    for(int i=0; i<9; i++){
        cin >> A[i];
    }
    for(int j=0; j<9; j++){
        if(A[j] > maxval){
            maxval = A[j];
            s = j;
        }
    }
    
    cout << maxval <<'\n';
    cout << s + 1;
}
